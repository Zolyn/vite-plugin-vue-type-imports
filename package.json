        "eslint": "^8.12.0",
        "eslint-config-alloy-patched": "^1.1.3",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-jest": "^25.7.0",
        "eslint-plugin-prettier": "^4.0.0",
        "esno": "0.14.1",
        "husky": "^8.0.0",
        "jest": "^27.5.1",
        "lint-staged": "^12.5.0",
        "prettier": "^2.6.2",
        "release-it": "^14.14.0",
        "sort-package-json": "^1.57.0",
